{
    "name": "OneOm",
    "description": "Helps OneOm work",
    "version": "0.2",
    "permissions": 
    [
        "background", 
        "webRequest", 
        "webRequestBlocking", 
        "*://oneom.tk/*",
        "*://vodlocker.com/*"
    ],
    "content_scripts": 
    [
        {
            "matches": ["*://oneom.tk/*", "*://vodlocker.com/*"],
            "js": ["jquery.js", "index.js"]
        }
    ],
    "externally_connectable": {
        "matches": ["*://oneom.tk/*"]
    },
    "icons": { 
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png" 
    },
    "background": 
    {
        "scripts": ["background.js"],
        "persistent": true
    },
    "manifest_version": 2
}

